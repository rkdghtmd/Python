#7번 문제
=====================================================
    number=int(input())
for i in range(number):
    count=0
    string=list(map(int,input().split()))
    average=sum(string[1:])/len(string[1:])
    for i in string[1:]:
        if i >average:
            count+=1 
    N=count/len(string[1:])*100
    print(f'{N:.3f}%')
