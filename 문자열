#5번 문제
==================================================================
string=list(input().upper())
a_list=[]
b_list=[]
for i in set(string):
  a_list.append(string.count(i))
for l in set(string):  
  if max(a_list)==string.count(l):
    b_list.append(l)
if len(b_list)==1:
    print(b_list[0])
else:
  print("?")
==================================================================
l=0
count=0
answer=""
string=input().upper()
for i in range(ord("A"),ord("Z")+1):
    if l <string.count(chr(i)):
        l =	 string.count(chr(i))
for k in range(ord("A"),ord("Z")):
    if string.count(chr(k))==l:
        count+=1
        answer=k
if count==1:
    print(chr(answer))
else:
    print("?")
==================================================================
string=input().upper()
l=0
count=0
for i in set(string):
    if string.count(str(l)) < string.count(i):
        l=i
for i in set(string):
    if string.count(i)==string.count(l):
        count+=1
if count==1:
    print(l)
else:
    print("?")
