#3번 문제
================================================================
N,M=map(int,input().split())
answer_list=[]
number_list=[i for i in range(1,1+N)]
check_list=[False]*N
def function(number):
    if number==M:
        print(*answer_list)
        return
    for i in range(N):
        if check_list[i]:
            continue
        answer_list.append(number_list[i])
        function(number+1)
        answer_list.pop()
function(0)

